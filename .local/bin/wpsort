#!/bin/sh
resolutions="1280x800 1366x768 1920x1080 1920x1200 1920x1280 2560x1440 2560x1600 3840x2160 3840x1080"

reztable=$(identify  -format "%f|%wx%h\n" ./*jpg ./*png 2>/dev/null)

for res in $resolutions; do
    mvlist=$(echo "$reztable" | grep "|$res\$" | cut -d '|' -f 1)

    [ -n "$mvlist" ] && mkdir -pv "$res" && while read -r f; do
            mv -v "$f" "$res"
    done <<EOT
$mvlist
EOT

done # for

exit 0
