macro v set browser "mpvurl    %u" ; open-in-browser ; set browser "$BROWSER"
macro V set browser "mpvurl -d %u" ; open-in-browser ; set browser "$BROWSER"

macro t set browser "ytthumb   %u" ; open-in-browser ; set browser "$BROWSER"
macro T set browser "ytthumb -m %u"; open-in-browser ; set browser "$BROWSER"

macro d set browser "rssdl   %u" ; open-in-browser ; set browser "$BROWSER"

macro m set browser "ytmusic %u &" ; open-in-browser ; set browser "$BROWSER"

macro y set browser "lynx %u"      ; open-in-browser ; set browser "$BROWSER"

macro ? set browser "pause"        ; open-in-browser ; set browser "$BROWSER"
